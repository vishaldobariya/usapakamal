{
    "extends": "stylelint-config-sass-guidelines",
    "plugins": [
        "stylelint-scss"
    ],
    "rules": {
        "scss/selector-no-redundant-nesting-selector": true,
        "max-nesting-depth": 2,
        "number-leading-zero": "never",
        "selector-no-qualifying-type": null
    }
}
